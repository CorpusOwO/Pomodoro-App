{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended",
        "standard",
        "eslint-config-prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "error",
        "quotes": ["error", "single"],
        "semi": ["error", "never"],
        "react/prop-types": "off"
    }
}
